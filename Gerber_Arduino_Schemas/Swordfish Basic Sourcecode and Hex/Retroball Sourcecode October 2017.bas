                        BallDelayXReset(0) = 1
                    ElseIf GameMode = 1 Then 
                        BallDelayXReset(0) = 0
                    EndIf
                    If IsPlaying(0) = false Then                        
                        BallsLeft(0) = true
                        BallsY(0) = 29   
                        BallsX(0) = 15  
                        BallDelayXReset(0) = 255   
                        If GameMode = 0 Then    
                            BallDelayYReset(0) = 1
                        ElseIf GameMode = 1 Then 
                            BallDelayYReset(0) = 0
                        EndIf
                    EndIf
                    BallDelayX(0) = 0
                    BallDelayY(0) = 0
                Else    
                    DisplayIndividualScore(0) = true 
