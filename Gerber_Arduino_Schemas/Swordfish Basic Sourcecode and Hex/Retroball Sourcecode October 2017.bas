' Sub Routines
Sub DisplayGraphics()
    For y = 0 To 15  
        For x = 0 To 31 
            RedData0 = OutputDataRed((15 - y) + 16).bits(31 - x)   ' the extra math in this code is required to rotate the screen so it will be the same orientation as the arduino
            RedData1 = OutputDataRed(15 - y).bits(31 - x) 
            GreenData0 = OutputDataGreen((15 - y) + 16).bits(31 - x)
            GreenData1 = OutputDataGreen(15 - y).bits(31 - x) 
            BlueData0 = OutputDataBlue((15 - y) + 16).bits(31 - x)
            BlueData1 = OutputDataBlue(15 - y).bits(31 - x)
            CLK = 1
            CLK = 0   
        Next
        OutputEnable = 1
        RowA = y.bits(0)
        RowB = y.bits(1)
        RowC = y.bits(2) 
        RowD = y.bits(3)  
        Latch = 1 
        Latch = 0 
        OutputEnable = 0
        DelayUS(LEDOnTime)
        OutputEnable = 1 
        DelayUS(LEDOffTime)
    Next
End Sub

Sub SetPixel(PixelX As Byte, PixelY As Byte, PixelColor As Byte)
    OutputDataRed(PixelX).bits(PixelY) = PixelColor.bits(0)  
    OutputDataGreen(PixelX).bits(PixelY) = PixelColor.bits(1)
    OutputDataBlue(PixelX).bits(PixelY) = PixelColor.bits(2)
End Sub

Sub DrawBox(FromX As Byte, FromY As Byte, ToX As Byte, ToY As Byte, Color As Byte)
    If toX < fromX Then
        TempByte = fromX
        fromX = toX
        toX = TempByte
    EndIf
    If  ToY < FromY Then
        TempByte = fromY
        fromY = toY
        toY = TempByte
    EndIf
    For x = 0 To (ToX - FromX)
        For y = 0 To (ToY - FromY)
            SetPixel(FromX + x, FromY + y, Color)
        Next
    Next
End Sub

Sub ClearScreen()
    For x = 0 To 31
        OutputDataRed(x) = 0
        OutputDataGreen(x) = 0
        OutputDataBlue(x) = 0
    Next
End Sub

Sub CheckControls()
     For x = 0 To 3
        ADCData(x) = ADC.Read(x)
     Next
     ADCData(1) = 1023 - ADCData(1)
     ADCData(2) = 1023 - ADCData(2)

    For x = 0 To 3 ' maybe look at moving this code to it's own sub routine
        If DisplayIndividualScore(x) = true Then
            If DisplayIndividualScoreDelay(x) <> 0 Then
                Dec(DisplayIndividualScoreDelay(x))
            Else
                DisplayIndividualScoreDelay(x) = 20
                DisplayIndividualScore(x) = false
            EndIf
        EndIf         
    Next    
    If DisplayScore = true Then 
        If DisplayScorePauseTimer <> 0 Then
            Dec(DisplayScorePauseTimer)
        Else
            DisplayScorePauseTimer = 100
            DisplayScore = false
        EndIf
    EndIf
    
    If WhichMode = DisplayTitleScreen Then
        Select SecretModeCounter Mod 2 
            Case 0
                If ADCData(0) < 10 Then
                    Inc(SecretModeCounter)
                EndIf 
            Case 1
                If ADCData(0) > 1000 Then
                    Inc(SecretModeCounter)
                EndIf 
        EndSelect 
        If SecretModeCounter = 3 Then
            SecretModeCounter = 0
            WhichMode = SecretMenu
        EndIf  
    EndIf
    If WhichMode = PlayGame Then
        For x = 0 To 3  
            PlayerPaddleOffset(x) = (ADCData(x) / (1024 / 25))
        Next
    EndIf
    If WhichMode = DisplayGameSelect Then  
        GameMode = (ADCData(0) / (1023 / 3))  
        If GameMode > 2 Then                  
            GameMode = 2
        EndIf
        If GameMode = 2 Then
            NumberOfBalls = 4  
        Else
            NumberOfBalls = 0   
        EndIf
    EndIf  
    
    If WhichMode = DisplayPlayerSelect Then
        ConfigurationOfPlayers = (ADCData(0) / (1024 / 7))     
        If ConfigurationOfPlayers > 6 Then                   
            ConfigurationOfPlayers = 6
        EndIf
            IsPlaying(0) = true ' red is always playing
        Select ConfigurationOfPlayers
            Case 0
                IsPlaying(3) = true
                IsPlaying(2) = false
                IsPlaying(1) = false
                NumberOfPlayers = 0
            Case 1
                IsPlaying(3) = false
                IsPlaying(2) = true
                IsPlaying(1) = false 
                NumberOfPlayers = 0
            Case 2
                IsPlaying(3) = false
                IsPlaying(2) = false
                IsPlaying(1) = true 
                NumberOfPlayers = 0
            Case 3
                IsPlaying(3) = true
                IsPlaying(2) = true
                IsPlaying(1) = false
                NumberOfPlayers = 1 
            Case 4
                IsPlaying(3) = true
                IsPlaying(2) = false
                IsPlaying(1) = true 
                NumberOfPlayers = 1
            Case 5
                IsPlaying(3) = false
                IsPlaying(2) = true
                IsPlaying(1) = true 
                NumberOfPlayers = 1
            Case 6
                IsPlaying(3) = true
                IsPlaying(2) = true
                IsPlaying(1) = true 
                NumberOfPlayers = 2
        End Select
    EndIf 
End Sub

Sub CheckPaddleBall()
    If WhichMode = PlayGame Then    
        For x = 0 To NumberOfBalls
            If IsPlaying(0) = false Then 
               If BallsX(x) = 1 And BallsUp(x) = true Then         
                  BallsUp(x) = false
               EndIf
            Else         
                TempData2 = PlayerPaddleOffset(0)
                TempData = Paddle(TempData2)
                If BallsX(x) = 1 And BallsUp(x) = true Then    
                    If TempData.bits(31 - BallsY(x)) = 1 Or ((TempData.bits(31 - BallsY(x) + 1) = 1 Or TempData.bits(31 - BallsY(x) - 1) = 1) And BallDelayYReset(x) <> 255) Then 
                        BallsUp(x) = false
                        BallDelayX(x) = 0
                        LastHitSpeed(0) = CurrentSpeed(0)                             
                            If CurrentSpeed(0) = 0 Then  
                                BallDelayYReset(x) = BallDelayYReset(x)
                            ElseIf PaddleDirection(0) = Left Then  
                                BallsLeft(x) = false 
                                BallDelayYReset(x) = (3 - CurrentSpeed(0)) 
                                BallDelayY(x) = 0  
                            ElseIf PaddleDirection(0) = Right Then   
                                BallsLeft(x) = true 
                                BallDelayYReset(x) = (3 - CurrentSpeed(0)) 
                                BallDelayY(x) = 0   
                            EndIf
                        EndIf
                    EndIf
                EndIf
            If IsPlaying(2) = false Then
               If BallsX(x) = 30 And BallsUp(x) = false Then
                  BallsUp(x) = true
               EndIf
            Else       
                TempData2 = PlayerPaddleOffset(2)
                TempData = Paddle(TempData2)
                If BallsX(x) = 30 And BallsUp(x) = false Then
                    If TempData.bits(31 - BallsY(x)) = 1 Or ((TempData.bits(31 - BallsY(x) + 1) = 1 Or TempData.bits(31 - BallsY(x) - 1) = 1) And BallDelayYReset(x) <> 255) Then
                        BallsUp(x) = true
                        BallDelayX(x) = 0
                        LastHitSpeed(2) = CurrentSpeed(2)                             
                        If CurrentSpeed(2) = 0 Then 
                            BallDelayYReset(x) = BallDelayYReset(x)
                        ElseIf PaddleDirection(2) = Left Then   
                            BallsLeft(x) = false 
                            BallDelayYReset(x) = (3 - CurrentSpeed(2)) 
                            BallDelayY(x) = 0   
                        ElseIf PaddleDirection(2) = Right Then   
                            BallsLeft(x) = true 
                            BallDelayYReset(x) = (3 - CurrentSpeed(2)) 
                            BallDelayY(x) = 0   
                        EndIf
                    EndIf
                EndIf
            EndIf                                                            
            If IsPlaying(1) = false Then 
               If BallsY(x) = 30 And BallsLeft(x) = false Then  
                  BallsLeft(x) = true
               EndIf
            Else      
                TempData2 = PlayerPaddleOffset(1)
                TempData = Paddle(TempData2)
                If BallsY(x) = 30 And BallsLeft(x) = false Then    
                    If TempData.bits(BallsX(x)) = 1 Or ((TempData.bits(BallsX(x) + 1) = 1 Or TempData.bits(BallsX(x) - 1) = 1) And BallDelayXReset(x) <> 255) Then 
                        BallsLeft(x) = true
                        BallDelayY(x) = 0
                        LastHitSpeed(1) = CurrentSpeed(1)                             
                        If CurrentSpeed(1) = 0 Then 
                            BallDelayXReset(x) = BallDelayXReset(x)
                        ElseIf PaddleDirection(1) = Left Then  
                            BallsUp(x) = false 
                            BallDelayXReset(x) = (3 - CurrentSpeed(1)) 
                            BallDelayX(x) = 0   
                        ElseIf PaddleDirection(1) = Right Then   
                            BallsUp(x) = true 
                            BallDelayXReset(x) = (3 - CurrentSpeed(1)) 
                            BallDelayX(x) = 0  
                        EndIf
                    EndIf
                EndIf
            EndIf
            If IsPlaying(3) = false Then 
               If BallsY(x) = 1 And BallsLeft(x) = true Then        
                  BallsLeft(x) = false
               EndIf
            Else          
                TempData2 = PlayerPaddleOffset(3)
                TempData = Paddle(TempData2)
                If BallsY(x) = 1 And BallsLeft(x) = true Then   
                    If TempData.bits(BallsX(x)) = 1 Or ((TempData.bits(BallsX(x) + 1) = 1 Or TempData.bits(BallsX(x) - 1) = 1) And BallDelayXReset(x) <> 255) Then
                        BallsLeft(x) = false
                        BallDelayY(x) = 0
                        LastHitSpeed(3) = CurrentSpeed(3)                             
                        If CurrentSpeed(3) = 0 Then    
                            BallDelayXReset(x) = BallDelayXReset(x)
                        ElseIf PaddleDirection(3) = Left Then   
                            BallsUp(x) = false 
                            BallDelayXReset(x) = (3 - CurrentSpeed(3)) 
                            BallDelayX(x) = 0  
                        ElseIf PaddleDirection(3) = Right Then   
                            BallsUp(x) = true 
                            BallDelayXReset(x) = (3 - CurrentSpeed(3)) 
                            BallDelayX(x) = 0  
                        EndIf
                    EndIf
                EndIf
            EndIf
        Next
    EndIf
End Sub

Sub UpdateBalls()
    If WhichMode = PlayGame And DisplayScore = false Then   
        For x = 0 To NumberOfBalls 
            If BallDelayX(x) <> 0 Then
               Dec(BallDelayX(x))
            Else
                BallDelayX(x) = BallDelayXReset(x)
                If BallsUp(x) = true Then
                   Dec(BallsX(x))
                Else
                   Inc(BallsX(x))
                EndIf
             EndIf   
            If BallDelayY(x) <> 0 Then
                Dec(BallDelayY(x))
            Else
                BallDelayY(x) = BallDelayYReset(x) 
                If BallsLeft(x) = true Then
                   Dec(BallsY(x))
                Else
                    Inc(BallsY(x))
                EndIf
            EndIf
        Next
    EndIf
End Sub

Sub SaveGameGraphicData()
    If WhichMode = PlayGame Then
        For y = 0 To 31         
            OutputDataRed(y) = 0
            OutputDataGreen(y) = 0
            OutputDataBlue(y) = 0
        Next
        OutputDataRed(0) = %10000000000000000000000000000001           
        OutputDataRed(31) = %10000000000000000000000000000001
        OutputDataGreen(0) = %10000000000000000000000000000001           
        OutputDataGreen(31) = %10000000000000000000000000000001
        OutputDataBlue(0) = %10000000000000000000000000000001           
        OutputDataBlue(31) = %10000000000000000000000000000001       
        TempData2 = PlayerPaddleOffset(1)
        SidePaddle1 = Paddle(TempData2)        
        TempData2 = PlayerPaddleOffset(3)
        SidePaddle2 = Paddle(TempData2)
        For x = 0 To 31
            If (DisplayScore = true Or DisplayIndividualScore(0) = true Or DisplayIndividualScore(2) = true) And x >= 0 And x <= 4 Then 
                If (IsPlaying(0) = true And NumberOfBalls = 0) Or DisplayIndividualScore(0) = true Then 
                    TempData = NumbersData0((((Scores(0) / 10) Mod 10) * 5) + x) << 17
                    TempData3 = NumbersData0(((Scores(0) Mod 10) * 5) + x) << 13
                    OutputDataRed(7 - x) = TempData Or TempData3 Or OutputDataRed(7 - x)
                EndIf
                If (IsPlaying(2) = true And NumberOfBalls = 0) Or DisplayIndividualScore(2) = true Then   
                    TempData = NumbersData2((((Scores(2) / 10) Mod 10) * 5) + x) << 13
                    TempData3 = NumbersData2(((Scores(2) Mod 10) * 5) + x) << 17
                    OutputDataGreen(28 - x) = TempData Or TempData3 Or OutputDataGreen(28 - x)
                EndIf
            EndIf
            If (DisplayScore = true Or DisplayIndividualScore(1) = true Or DisplayIndividualScore(3) = true) And x >= 0 And x <= 2 Then
                If (IsPlaying(1) = true And NumberOfBalls = 0) Or DisplayIndividualScore(1) = true Then  
                    TempData = NumbersData1((((Scores(1) / 10) Mod 10) * 3) + x)
                    TempData3 = NumbersData1(((Scores(1) Mod 10) * 3) + x) 
                    OutputDataBlue(14 - x) = TempData Or OutputDataBlue(14 - x)  
                    OutputDataBlue(18 - x) = TempData3 Or OutputDataBlue(18 - x)   
                EndIf
                If (IsPlaying(3) = true And NumberOfBalls = 0) Or DisplayIndividualScore(3) = true Then   
                    TempData = NumbersData3(((Scores(3) Mod 10) * 3) + x) << 21
                    TempData3 = NumbersData3((((Scores(3) / 10) Mod 10) * 3) + x) << 21 
                    OutputDataRed(14 - x) = TempData Or OutputDataRed(14 - x)  
                    OutputDataRed(18 - x) = TempData3 Or OutputDataRed(18 - x) 
                    OutputDataGreen(14 - x) = TempData Or OutputDataGreen(14 - x)  
                    OutputDataGreen(18 - x) = TempData3 Or OutputDataGreen(18 - x) 
                EndIf
            EndIf
            For y = 0 To NumberOfBalls ' upto 5 balls 
                If BallsX(y) = x Then
                    If BallDelayXReset(y) = 0 Or BallDelayYReset(y) = 0 Then
                        OutputDataRed(x) = OutputDataRed(x) Or Balls(BallsY(y)) ' red 
                    ElseIf BallDelayXReset(y) = 1 Or BallDelayYReset(y) = 1 Then
                        OutputDataGreen(x) = OutputDataGreen(x) Or Balls(BallsY(y)) ' orange
                        OutputDataRed(x) = OutputDataRed(x) Or Balls(BallsY(y)) ' 
                    ElseIf BallDelayXReset(y) = 2 Or BallDelayYReset(y) = 2 Then
                        OutputDataGreen(x) = OutputDataGreen(x) Or Balls(BallsY(y)) ' green  
                    ElseIf BallDelayXReset(y) = 3 Or BallDelayYReset(y) = 3 Then
                        OutputDataBlue(x) = OutputDataBlue(x) Or Balls(BallsY(y)) ' blue
                    ElseIf BallDelayXReset(y) = 4 Or BallDelayYReset(y) = 4 Then
                        OutputDataBlue(x) = OutputDataBlue(x) Or Balls(BallsY(y)) ' cyan
                        OutputDataGreen(x) = OutputDataGreen(x) Or Balls(BallsY(y)) '
                    EndIf   
                EndIf
            Next
            If x = 0 And IsPlaying(0) = true Then
               TempData2 = PlayerPaddleOffset(0)
               OutputDataRed(x) = OutputDataRed(x) Or Paddle(TempData2)
            ElseIf x = 0 And IsPlaying(0) = false Then
                 OutputDataRed(x) = %11111111111111111111111111111111
            EndIf
            If x = 31 And IsPlaying(2) = true Then
               TempData2 = PlayerPaddleOffset(2)
               OutputDataGreen(x) = OutputDataGreen(x) Or Paddle(TempData2)
            ElseIf x = 31 And IsPlaying(2) = false Then
                 OutputDataGreen(x) = %11111111111111111111111111111111
            EndIf
            If x > 0 And x < 31 Then
               If IsPlaying(1) = true Then
                   If SidePaddle1.bits(x) = 1 Then  
                      OutputDataBlue(x).bits(0) = 1
                   EndIf
               Else
                   OutputDataBlue(x).bits(0) = 1
               EndIf
               If IsPlaying(3) = true Then
                   If SidePaddle2.bits(x) = 1 Then  
                      OutputDataRed(x).bits(31) = 1
                      OutputDataGreen(x).bits(31) = 1
                   EndIf
               Else
                   OutputDataRed(x).bits(31) = 1
                   OutputDataGreen(x).bits(31) = 1
               EndIf 
            EndIf
        Next 
    EndIf
End Sub

Sub SaveMenuGraphicData() 
    If WhichMode = SecretMenu Then
        SecretMenuScreenNumber = ADCData(0) / (1023 / ((Bound(SecretMenuScreensRed) + 32) / 32))
        If SecretMenuScreenNumber > 8 Then
            SecretMenuScreenNumber = 8
        EndIf
        For x = 0 To 31
            OutputDataRed(x) = SecretMenuScreensRed(x + (SecretMenuScreenNumber * 32)) 
            OutputDataGreen(x) = SecretMenuScreensGreen(x + (SecretMenuScreenNumber * 32))
            OutputDataBlue(x) = SecretMenuScreensBlue(x + (SecretMenuScreenNumber * 32))
        Next
    EndIf
    
    If WhichMode = ScreenTest Then
        TempData2 = ADCData(0) / 140
        If TempData2 > 7 Then
            TempData2 = 7
        EndIf
        For x = 0 To 31
            If TempData2.bits(0) = 1 Then
                OutputDataRed(x) = %11111111111111111111111111111111
            Else
                OutputDataRed(x) = 0
            EndIf   
            If TempData2.bits(1) = 1 Then
                OutputDataGreen(x) = %11111111111111111111111111111111
            Else
                OutputDataGreen(x) = 0
            EndIf   
            If TempData2.bits(2) = 1 Then
                OutputDataBlue(x) = %11111111111111111111111111111111
            Else
                OutputDataBlue(x) = 0
            EndIf    
        Next         
    EndIf
    
    If WhichMode = PotTest Then
        ClearScreen()
        Scores(0) = ADCData(0) / 10.2
        Scores(1) = (1023 - ADCData(1)) / 10.2
        Scores(2) = (1023 - ADCData(2)) / 10.2
        Scores(3) = ADCData(3) / 10.2
        For x = 0 To 3
            If Scores(x) > 98 Then
                Scores(x) = 99
            EndIf
        Next
        For x = 0 To 31
            If x >= 0 And x <= 4 Then  
                TempData = NumbersData0((((Scores(0) / 10) Mod 10) * 5) + x) << 17
                TempData3 = NumbersData0(((Scores(0) Mod 10) * 5) + x) << 13
                OutputDataRed(7 - x) = TempData Or TempData3 Or OutputDataRed(7 - x)
                  
                TempData = NumbersData2((((Scores(2) / 10) Mod 10) * 5) + x) << 13
                TempData3 = NumbersData2(((Scores(2) Mod 10) * 5) + x) << 17
                OutputDataGreen(28 - x) = TempData Or TempData3 Or OutputDataGreen(28 - x)
            EndIf
            If x >= 0 And x <= 2 Then 
                TempData = NumbersData1((((Scores(1) / 10) Mod 10) * 3) + x)
                TempData3 = NumbersData1(((Scores(1) Mod 10) * 3) + x) 
                OutputDataBlue(14 - x) = TempData Or OutputDataBlue(14 - x)  
                OutputDataBlue(18 - x) = TempData3 Or OutputDataBlue(18 - x)   
                   
                TempData = NumbersData3(((Scores(3) Mod 10) * 3) + x) << 21
                TempData3 = NumbersData3((((Scores(3) / 10) Mod 10) * 3) + x) << 21 
                OutputDataRed(14 - x) = TempData Or OutputDataRed(14 - x)  
                OutputDataRed(18 - x) = TempData3 Or OutputDataRed(18 - x) 
                OutputDataGreen(14 - x) = TempData Or OutputDataGreen(14 - x)  
                OutputDataGreen(18 - x) = TempData3 Or OutputDataGreen(18 - x) 
            EndIf
        Next
    EndIf
    
    If WhichMode = LEDSketch Then
        OutputDataRed(ADCData(0) / 32).bits(31 - (ADCData(3) / 32)) = PenColor.bits(0)
        OutputDataGreen(ADCData(0) / 32).bits(31 - (ADCData(3) / 32)) = PenColor.bits(1)
        OutputDataBlue(ADCData(0) / 32).bits(31 - (ADCData(3) / 32)) = PenColor.bits(2)    
    EndIf
    
    If WhichMode = ScreenSaver1 Then
        DrawBox(Rand, Rand, Rand, Rand, Rand)
    EndIf
    
    If WhichMode = ScreenSaver2 Then
        SetPixel(Rand, Rand, Rand)
    EndIf
    
    If WhichMode = Dice Then
        ClearScreen()
        If RollDice = true Then
            Dec(DiceRollTime)
            Scores(0) = Rand() / 5
            If Scores(0) > 5 Then
                Scores(0) = 5
            EndIf
            DiceColor = Rand() / 4
            If DiceColor = 0 Or DiceColor > 7 Then ' we just check to make sure the color will actually be a color we can see!
                DiceColor = 1
            EndIf
            If DiceRollTime = 0 Then
                RollDice = false
            EndIf
        EndIf
        For x = 0 To 31
            If DiceColor.bits(0) = 1 Then
                OutputDataRed(x) = DiceNumbers(((Scores(0)) * 32) + 31 - x)
            EndIf
            If DiceColor.bits(1) = 1 Then
                OutputDataGreen(x) = DiceNumbers(((Scores(0)) * 32) + 31 - x)
            EndIf
            If DiceColor.bits(2) = 1 Then
                OutputDataBlue(x) = DiceNumbers(((Scores(0)) * 32) + 31 - x)
            EndIf
        Next
    EndIf
    
    If WhichMode = GreatRace Then
        CarLocation = 31 - (ADCData(0) / 31)
        If CarLocation > 29 Then
            CarLocation = 29
        EndIf   
        If GameSpeed <> 0 Then
            Dec(GameSpeed)
        Else
            GameSpeed = (1023 - ADCData(3)) / 204
            Inc(TrackOffset)
            If TrackOffset = Bound(Track1Red) - 32 Then
                TrackOffset = 0
            EndIf
        EndIf
    
        For x = 0 To 31
            OutputDataRed(x) = Track1Red(x + TrackOffset)
            OutputDataGreen(x) = Track1Green(x + TrackOffset)
            OutputDataBlue(x) = Track1Blue(x + TrackOffset)
        Next
        ' now that we have the trackdata in our displaymemory - we can check for a collision by using the display memory.
        For x = 0 To 3
            TempData = OutputDataGreen(x)
            For y = 0 To 2
                If TempData.bits(CarLocation + y) = 1 Then
                    TrackOffset = 0
                EndIf
            Next
        Next
        ' and now let's draw the car on the screen
        OutputDataRed(0).bits(CarLocation + 1) = 1
        OutputDataRed(1).bits(CarLocation + 1) = 1 
        OutputDataRed(2).bits(CarLocation + 1) = 1
        OutputDataRed(3).bits(CarLocation + 1) = 1
        OutputDataRed(0).bits(CarLocation + 2) = 1
        OutputDataGreen(0).bits(CarLocation + 2) = 1
        OutputDataBlue(0).bits(CarLocation + 2) = 1 
        OutputDataRed(0).bits(CarLocation) = 1
        OutputDataGreen(0).bits(CarLocation) = 1
        OutputDataBlue(0).bits(CarLocation) = 1 
        OutputDataRed(2).bits(CarLocation + 2) = 1
        OutputDataGreen(2).bits(CarLocation + 2) = 1
        OutputDataBlue(2).bits(CarLocation + 2) = 1 
        OutputDataRed(2).bits(CarLocation) = 1
        OutputDataGreen(2).bits(CarLocation) = 1
        OutputDataBlue(2).bits(CarLocation) = 1 
    EndIf
    
    If WhichMode = 100 Then
        OutputDataRed(0).bits(CarLocation) = 0
        CarLocation = 32 - (ADCData(0) / 32)
        If CarLocation > 32 Then
            CarLocation = 32
        EndIf  
        OutputDataBlue(0).bits(CarLocation2) = 0
        CarLocation2 = 32 - (ADCData(3) / 32)
        If CarLocation2 > 32 Then
            CarLocation2 = 32
        EndIf
        If GameSpeed <> 0 Then
            Dec(GameSpeed)
        Else
            GameSpeed = 1
            Inc(TrackOffset)
            If TrackOffset = Bound(Track1Red) - 32 Then
                TrackOffset = 0
            EndIf
        EndIf    
        For x = 0 To 31
            OutputDataRed(x) = Track1Red(x + TrackOffset)
            OutputDataGreen(x) = Track1Green(x + TrackOffset)
            OutputDataBlue(x) = Track1Blue(x + TrackOffset)
        Next
        OutputDataRed(0).bits(CarLocation) = 1
        If OutputDataGreen(0).bits(CarLocation) = 1 Then
            'TrackOffset = 0
        EndIf
        OutputDataBlue(0).bits(CarLocation2) = 1
        If OutputDataGreen(0).bits(CarLocation2) = 1 Then
            'TrackOffset = 0
        EndIf
    EndIf
    
    If WhichMode = DisplayGameSelect Or WhichMode = DisplayPlayerSelect Then
        If MenuScrollDelay <> 0 Then
            Dec(MenuScrollDelay)
        Else
            MenuScrollDelay = 5
            If MenuScrollUp = true Then
                Inc(z)
                If z = 11 Then
                    z = 10
                    MenuScrollUp = false
                EndIf
            ElseIf MenuScrollUp = false Then
                Dec(z)
                If z = 255 Then
                    z = 0
                    MenuScrollUp = true
                EndIf
            EndIf    
        EndIf 
        OutputDataRed(0) = 0
        OutputDataRed(1) = 0
        OutputDataRed(2) = 0
        For y = 0 To 31
            If y < 11 And y > 2 Then    
                If WhichMode = DisplayGameSelect Then 
                    OutputDataRed(13 - y) = SelectMode((y - 3) + z)
                    OutputDataGreen(13 - y) = SelectMode((y - 3) + z)
                    OutputDataBlue(13 - y) = SelectMode((y - 3) + z)  
                ElseIf WhichMode = DisplayPlayerSelect Then
                    OutputDataRed(13 - y) = SelectPlayers((y - 3) + z)
                    OutputDataGreen(13 - y) = SelectPlayers((y - 3) + z)
                    OutputDataBlue(13 - y) = SelectPlayers((y - 3) + z)
                    ' we always have a red arrow
                    OutputDataRed(2) = %00000000000001111100000000000000
                    OutputDataRed(1) = %00000000000000111000000000000000
                    OutputDataRed(0) = %00000000000000010000000000000000
                EndIf
            ElseIf y > 10 Then   
                If WhichMode = DisplayGameSelect Then
                    OutputDataRed(y) = GameModesData((GameMode * 32) + 31 - y)
                    OutputDataGreen(y) = GameModesData((GameMode * 32) + 31 - y)
                    OutputDataBlue(y) = GameModesData((GameMode * 32) + 31 - y)   
                ElseIf WhichMode = DisplayPlayerSelect Then
                    OutputDataRed(y) = BigNumbers(((NumberOfPlayers + 1) * 32) + 31 - y) 
                    OutputDataGreen(y) = BigNumbers(((NumberOfPlayers + 1) * 32) + 31 - y) 
                    OutputDataBlue(y) = BigNumbers(((NumberOfPlayers + 1) * 32) + 31 - y) 
                    ' we have a blue arrow IF blue is going to be playing
                    If IsPlaying(1) = true Then
                        OutputDataBlue(17) = OutputDataBlue(17) Or %00000000000000000000000000000100
                        OutputDataBlue(16) = OutputDataBlue(16) Or %10000000000000000000000000000110
                        OutputDataBlue(15) = OutputDataBlue(15) Or %10000000000000000000000000000111
                        OutputDataBlue(14) = OutputDataBlue(14) Or %10000000000000000000000000000110 
                        OutputDataBlue(13) = OutputDataBlue(13) Or %00000000000000000000000000000100
                        ' I don't understand why but when you OR together two longwords you actually delete all data from the longword you are saving to IF
                        ' the longword you are trying to OR into it does not have a logic 1 in bit 31 and it has a logic 1 in bit 0 or bit 1. It is very strange. to fix it I have had to
                        ' put extra logic 1's in the data about (bit 31) to make sure it does not delete data, but then I need to individually erase these bits as seen below
                        OutputDataBlue(16).bits(31) = 0 
                        OutputDataBlue(15).bits(31) = 0 
                        OutputDataBlue(14).bits(31) = 0
                    EndIf
                    ' we have a green arrow IF green is going to be playing
                    If IsPlaying(2) = true Then
                        OutputDataGreen(31) = OutputDataGreen(31) Or %00000000000000010000000000000000
                        OutputDataGreen(30) = OutputDataGreen(30) Or %00000000000000111000000000000000
                        OutputDataGreen(29) = OutputDataGreen(29) Or %00000000000001111100000000000000 
                    EndIf
                    ' we have a yellow arrow IF yellow is going to be playing
                    If IsPlaying(3) = true Then
                        OutputDataRed(17) = OutputDataRed(17) Or %00100000000000000000000000000000
                        OutputDataRed(16) = OutputDataRed(16) Or %01100000000000000000000000000000
                        OutputDataRed(15) = OutputDataRed(15) Or %11100000000000000000000000000000 
                        OutputDataRed(14) = OutputDataRed(14) Or %01100000000000000000000000000000 
                        OutputDataRed(13) = OutputDataRed(13) Or %00100000000000000000000000000000 
                        OutputDataGreen(17) = OutputDataGreen(17) Or %00100000000000000000000000000000
                        OutputDataGreen(16) = OutputDataGreen(16) Or %01100000000000000000000000000000
                        OutputDataGreen(15) = OutputDataGreen(15) Or %11100000000000000000000000000000 
                        OutputDataGreen(14) = OutputDataGreen(14) Or %01100000000000000000000000000000 
                        OutputDataGreen(13) = OutputDataGreen(13) Or %00100000000000000000000000000000 
                    EndIf
                EndIf
            EndIf
        Next
    EndIf
End Sub

Sub SaveTitleScreenData()
    If WhichMode = DisplayTitleScreen Then
        For x = 0 To 31 
            OutputDataRed(x) = 0
            OutputDataGreen(x) = 0
            OutputDataBlue(x) = 0
        Next
        For x = 0 To 8
            OutputDataRed(31 - TitleSuperY - x) = (TitleSuperData(x) >> TitleSuperX) 
            OutputDataRed(31 - TitlePongY - x) = OutputDataRed(31 - TitlePongY - x) Or (TitlePongData(x) >> TitlePongX)
            OutputDataGreen(31 - TitleSuperY - x) = (TitleSuperData(x) >> TitleSuperX) 
            OutputDataGreen(31 - TitlePongY - x) = OutputDataRed(31 - TitlePongY - x) Or (TitlePongData(x) >> TitlePongX)
            OutputDataBlue(31 - TitleSuperY - x) = (TitleSuperData(x) >> TitleSuperX) 
            OutputDataBlue(31 - TitlePongY - x) = OutputDataRed(31 - TitlePongY - x) Or (TitlePongData(x) >> TitlePongX)
        Next
        If TitleShiftDelay <> 0 Then
            Dec(TitleShiftDelay)
        Else
            TitleShiftDelay = 5
            If TitleSuperMovingLeft = true Then
                Inc(TitleSuperX)
                If TitleSuperX = 7 Then
                    TitleSuperMovingLeft = false
                EndIf
            Else
                Dec(TitleSuperX)
                If TitleSuperX = 0 Then
                    TitleSuperMovingLeft = true
                EndIf
            EndIf
            If TitleSuperMovingDown = true Then
                Inc(TitleSuperY)
                If (TitleSuperY + 9) = TitlePongY Then
                    TitleSuperMovingDown = false
                    TitlePongMovingDown = true
                EndIf
            Else
                Dec(TitleSuperY)
                If TitleSuperY = 0 Then
                    TitleSuperMovingDown = true
                EndIf
            EndIf
            
            If TitlePongMovingLeft = true Then
                Inc(TitlePongX)
                If TitlePongX = 13 Then
                    TitlePongMovingLeft = false
                EndIf
            Else
                Dec(TitlePongX)
                If TitlePongX = 0 Then
                    TitlePongMovingLeft = true
                EndIf
            EndIf
            If TitlePongMovingDown = true Then
                Inc(TitlePongY)
                If TitlePongY = 23 Then
                    TitlePongMovingDown = false
                EndIf
            Else
                Dec(TitlePongY)
                If (TitlePongY - 9) = TitleSuperY Then
                    TitlePongMovingDown = true
                    TitleSuperMovingDown = false
                EndIf
            EndIf
            
        EndIf
    EndIf
End Sub

Sub SplitTransition()   
    If WhichMode = DoScreenTransition Then
        For y = 0 To 31
            OutputDataRed(y) = OutputDataRed(y) << 1    ' can change this to go in different directions
            OutputDataGreen(y) = OutputDataGreen(y) >> 1
            If y < 31 Then
                OutputDataBlue(y) = OutputDataBlue(y + 1)
                OutputDataBlue(y + 1) = 0
            EndIf
        Next
        Inc(ScreenTransitionCounter)
        If ScreenTransitionCounter = 32 Then
            ScreenTransitionCounter = 0
            If LastModeSave = DisplayTitleScreen Then
                WhichMode = DisplayPlayerSelect
            ElseIf LastModeSave = SecretMenu Then
                If SecretMenuScreenNumber = 0 Then
                    WhichMode = SecretMenu
                ElseIf SecretMenuScreenNumber = 1 Then
                    WhichMode = ScreenTest
                ElseIf SecretMenuScreenNumber = 2 Then
                    WhichMode = PotTest
                ElseIf SecretMenuScreenNumber = 3 Then
                    WhichMode = ScreenSaver1
                ElseIf SecretMenuScreenNumber = 4 Then
                    WhichMode = ScreenSaver2
                ElseIf SecretMenuScreenNumber = 5 Then
                    WhichMode = LEDSketch
                ElseIf SecretMenuScreenNumber = 6 Then
                    WhichMode = GreatRace
                    TrackOffset = 0
                ElseIf SecretMenuScreenNumber = 7 Then
                    WhichMode = Dice
                    RollDice = true
                    DiceRollTime = 40
                ElseIf SecretMenuScreenNumber = 8 Then
                    WhichMode = DisplayTitleScreen
                EndIf
            ElseIf LastModeSave = ScreenTest Or LastModeSave = PotTest Or LastModeSave = LEDSketch Or LastModeSave = ScreenSaver1 Or LastModeSave = ScreenSaver2 Or LastModeSave = Dice Or LastModeSave = GreatRace Then
                WhichMode = SecretMenu
            ElseIf LastModeSave = DisplayPlayerSelect Then
                WhichMode = DisplayGameSelect
            ElseIf LastModeSave = DisplayGameSelect Then
                WhichMode = PlayGame
                For x = 0 To 3
                    WinnerIsPlayer(x) = false  
                    If NumberOfBalls = 0 Then
                        Scores(x) = 11  
                        DisplayIndividualScore(x) = false    
                    Else
                        Scores(x) = 50  
                        DisplayIndividualScoreDelay(x) = 100  
                        DisplayIndividualScore(x) = true   
                        DisplayScore = true   
                    EndIf
                    If IsPlaying(x) = false Then
                        Scores(x) = 0
                        DisplayIndividualScore(x) = false
                    EndIf
                Next
                For x = 0 To 4
                    BallsX(x) = 15
                    BallsY(x) = 15
                Next
                If GameMode = 0 Or GameMode = 2 Then    
                    BallDelayXReset(0) = 1  
                ElseIf GameMode = 1 Then    
                    BallDelayXReset(0) = 0 
                EndIf
                If GameMode = 0 Or GameMode = 1 Then   
                    DisplayScore = true 
                    BallDelayYReset(0) = 255 
                    BallDelayX(0) = BallDelayXReset(0)
                    BallDelayY(0) = BallDelayYReset(0)
                    BallsUp(0) = false
                    BallsX(0) = 2   
                    BallsY(0) = 15
                EndIf
            ElseIf LastModeSave = PlayGame Then
                WhichMode = DisplayWinner
            ElseIf LastModeSave = DisplayWinner Then
                WhichMode = DisplayTitleScreen
            EndIf
        EndIf
    EndIf
End Sub

Sub CheckModeChange()   
    If WhichMode <> DoScreenTransition Then    
        If SelectButtonPressed = false Then   
            If SelectButton = 0 Or (WhichMode = DisplayWinner And WinScreenHoldTimer = 0) Then          
                WinScreenHoldTimer = 1000  
                SelectButtonPressed = true
                z = 0   
                LastModeSave = WhichMode
                If WhichMode = PlayGame Then
                    LastModeSave = DisplayWinner    
                EndIf
                WhichMode = DoScreenTransition
            EndIf
        EndIf
    EndIf      
End Sub

