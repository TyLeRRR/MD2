Sub CheckPaddleSpeed()
    For x = 0 To 3 
        If SpeedCheckDelay(x) <> 0 Then
            Dec(SpeedCheckDelay(x))
        Else
            SpeedCheckDelay(x) = 3
            OldPosition(x) = ADCData(x) / (1024 / 32)
        EndIf
        CurrentPosition(x) = ADCData(x) / (1024 / 32)
        If CurrentPosition(x) > OldPosition(x) Then
            If x = 0 Or x = 2 Then
                PaddleDirection(x) = Right 
            Else
                PaddleDirection(x) = Left
            EndIf
            CurrentSpeed(x) = CurrentPosition(x) - OldPosition(x)
            PaddleNotMovingDelay(x) = 10
        ElseIf CurrentPosition(x) < OldPosition(x) Then
            If x = 0 Or x = 2 Then
                PaddleDirection(x) = Left 
            Else
                PaddleDirection(x) = Right
            EndIf
            CurrentSpeed(x) = OldPosition(x) - CurrentPosition(x)
            PaddleNotMovingDelay(x) = 10
        Else
            If PaddleNotMovingDelay(x) <> 0 Then
                Dec(PaddleNotMovingDelay(x))
            Else
                CurrentSpeed(x) = 0  
                PaddleDirection(x) = 0
            EndIf
        EndIf
        If CurrentSpeed(x) > 3 Then   
            CurrentSpeed(x) = 3
        EndIf
    Next   
End Sub

