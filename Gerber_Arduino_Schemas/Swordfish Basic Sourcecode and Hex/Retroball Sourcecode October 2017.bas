                    IsPlaying(2) = false
                EndIf
                If GameMode = 0 Or GameMode = 1 Then 
                    DisplayScore = true
                    BallsUp(0) = true  
                    BallsX(0) = 29  
                    BallsY(0) = 15   
                    BallDelayYReset(0) = 255  
                    If GameMode = 0 Then    
                        BallDelayXReset(0) = 1
                    ElseIf GameMode = 1 Then 
                        BallDelayXReset(0) = 0
                    EndIf                 
                    If IsPlaying(2) = false Then  
                        BallsLeft(0) = false
                        BallsY(0) = 2   
                        BallsX(0) = 15   
                        BallDelayXReset(0) = 255    
                        If GameMode = 0 Then   
                            BallDelayYReset(0) = 1
                        ElseIf GameMode = 1 Then 
                            BallDelayYReset(0) = 0
                        EndIf
                    EndIf
                    BallDelayX(0) = 0
                    BallDelayY(0) = 0 
                Else
                    DisplayIndividualScore(2) = true
                    DoMultiBall = true
                EndIf
            ElseIf BallsY(x) = 31 And IsPlaying(1) = true Then 
                Dec(Scores(1))
                If Scores(1) = 0 Then
                    IsPlaying(1) = false
                EndIf
                If GameMode = 0 Or GameMode = 1 Then 
                    DisplayScore = true
                    BallsLeft(0) = true
                    BallsY(0) = 29   
                    BallsX(0) = 15  
                    BallDelayXReset(0) = 255    
                    If GameMode = 0 Then    
                        BallDelayYReset(0) = 1
                    ElseIf GameMode = 1 Then 
                        BallDelayYReset(0) = 0
                    EndIf
                    If IsPlaying(1) = false Then                        
                        BallsUp(0) = true  
                        BallsX(0) = 29   
                        BallsY(0) = 15  
                        BallDelayYReset(0) = 255    
                        If GameMode = 0 Then   
                            BallDelayXReset(0) = 1
                        ElseIf GameMode = 1 Then 
                            BallDelayXReset(0) = 0
                        EndIf
                    EndIf
                    BallDelayX(0) = 0
                    BallDelayY(0) = 0
                Else
                    DisplayIndividualScore(1) = true
                    DoMultiBall = true   
                EndIf
            ElseIf BallsY(x) = 0 And IsPlaying(3) = true Then 
                Dec(Scores(3))  
                If Scores(3) = 0 Then
                    IsPlaying(3) = false
                EndIf
                If GameMode = 0 Or GameMode = 1 Then  
                    DisplayScore = true
                    BallsLeft(0) = false
                    BallsY(0) = 2  
                    BallsX(0) = 15   
                    BallDelayXReset(0) = 255  
                    If GameMode = 0 Then    
                        BallDelayYReset(0) = 1
                    ElseIf GameMode = 1 Then 
                        BallDelayYReset(0) = 0
                    EndIf
                    If IsPlaying(3) = false Then                       
                        BallsUp(0) = false  
                        BallsX(0) = 2   
                        BallsY(0) = 15 
                        BallDelayYReset(0) = 255   
                        If GameMode = 0 Then    
                            BallDelayXReset(0) = 1
                        ElseIf GameMode = 1 Then 
                            BallDelayXReset(0) = 0
                        EndIf
                    EndIf
                    BallDelayX(0) = 0
                    BallDelayY(0) = 0
                Else
                    DisplayIndividualScore(3) = true
                    DoMultiBall = true   
                EndIf
            EndIf
            
            If DoMultiBall = true Then
                DoMultiBall = false
                RandomNumber = GetRND
                If RandomNumber.bits(0) = 1 Then
                    BallsLeft(x) = true
                Else
                    BallsLeft(x) = false
                EndIf 
                If RandomNumber.bits(1) = 1 Then
                    BallsUp(x) = true
                Else
                    BallsUp(x) = false
                EndIf
                If RandomNumber.bits(2) = 1 Then
                    BallDelayXReset(x) = 2
                Else
                    BallDelayXReset(x) = 1
                EndIf
                If RandomNumber.bits(3) = 1 Then
                    BallDelayYReset(x) = 2
                Else
                    BallDelayYReset(x) = 3
                EndIf
                If RandomNumber.bits(4) = 1 Then
                    BallDelayXReset(x) = 1
                Else
                    BallDelayYReset(x) = 4
                EndIf
                BallsX(x) = 15  
                BallsY(x) = 15
                If NumberOfPlayers = 0 Then
                    If RandomNumber.bits(5) = 1 Then
                        BallDelayXReset(x) = 1
                    Else 
                        BallDelayXReset(x) = 2
                    EndIf
                    If RandomNumber.bits(6) = 1 Then
                        BallDelayYReset(x) = 255
                    Else
                        BallDelayYReset(x) = 2
                    EndIf                        
                EndIf
                BallDelayX(x) = 0 
                BallDelayY(x) = 0  
            EndIf               
        Next
    EndIf
End Sub

Sub CheckIfWeHaveAWinner()
    If WhichMode = PlayGame Then
        If Scores(0) > 0 And Scores(1) = 0 And Scores(2) = 0 And Scores(3) = 0 Then  
            LastModeSave = WhichMode
            WhichMode = DoScreenTransition
            WinnerIsPlayer(0) = true
        ElseIf Scores(1) > 0 And Scores(0) = 0 And Scores(2) = 0 And Scores(3) = 0 Then   
            LastModeSave = WhichMode
            WhichMode = DoScreenTransition
            WinnerIsPlayer(1) = true
        ElseIf Scores(2) > 0 And Scores(0) = 0 And Scores(1) = 0 And Scores(3) = 0 Then  
            LastModeSave = WhichMode
            WhichMode = DoScreenTransition
            WinnerIsPlayer(2) = true
        ElseIf Scores(3) > 0 And Scores(0) = 0 And Scores(1) = 0 And Scores(2) = 0 Then  
            LastModeSave = WhichMode
            WhichMode = DoScreenTransition
            WinnerIsPlayer(3) = true
        EndIf
    EndIf
End Sub

Sub DisplayWinnerGraphics()
    If WhichMode = DisplayWinner Then
        Dec(WinScreenHoldTimer)
        If WinnerDelay <> 0 Then
            Dec(WinnerDelay)
        Else
            WinnerDelay = 3
            Inc(WinnerFrame)
            If WinnerFrame = 9 Then
                WinnerFrame = 0
            EndIf
        EndIf
        For x = 0 To 31
            If WinnerIsPlayer(0) = true Then
                OutputDataRed(31 - x) = YouWinData0(x + (WinnerFrame * 32))
            ElseIf WinnerIsPlayer(1) = true Then
                OutputDataBlue(31 - x) = YouWinData1(x + (WinnerFrame * 32))
            ElseIf WinnerIsPlayer(2) = true Then
                OutputDataGreen(31 - x) = YouWinData2(x + (WinnerFrame * 32))
            ElseIf WinnerIsPlayer(3) = true Then
                OutputDataRed(31 - x) = YouWinData3(x + (WinnerFrame * 32))
                OutputDataGreen(31 - x) = YouWinData3(x + (WinnerFrame * 32))
            EndIf
        Next
    EndIf
End Sub

Sub CheckBallWithinBounds()
    If WhichMode = PlayGame Then
        For x  = 0 To 4
            If BallsX(x) > 31 Or BallsY(x) > 31 Then
                BallsX(x) = 14
                BallsY(x) = 14
            EndIf
        Next
    EndIf
End Sub

Sub CheckPauseButton()
    If PauseButtonPressed = false And PauseButton = 0 Then
        PauseButtonPressed = true
        If WhichMode = PlayGame Then
            If GameIsPaused = false Then
                GameIsPaused = true
            Else
                GameIsPaused = false
            EndIf
        ElseIf WhichMode = LEDSketch Then
            Inc(PenColor)
            If PenColor > 7 Then
                PenColor = 1
            EndIf
        ElseIf WhichMode = Dice Then
            DiceRollTime = 40
            RollDice = true
        EndIf    
    EndIf
