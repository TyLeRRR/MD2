                Dec(Scores(0))
                If Scores(0) = 0 Then
                    IsPlaying(0) = false
                EndIf
                If GameMode = 0 Or GameMode = 1 Then 
                    DisplayScore = true
                    BallsUp(0) = false  
                    BallsX(0) = 2  
                    BallsY(0) = 15 
                    BallDelayYReset(0) = 255    
